{
  "extends": "tsconfig-kodefox",
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmit": true,
    "lib": ["dom", "esnext"],
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "types": ["react", "react-native"],
  "include": ["src"]
}
